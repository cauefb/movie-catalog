<h1><%= @director.name %></h1>
<p><strong>Nationality:</strong> <%= @director.nationality %></p>
<p><strong>Birth Date:</strong> <%= @director.birth_date.strftime("%d/%m/%Y") %></p>
<p><strong>Favorite Genre:</strong> <%= @director.favorite_genre %></p>
<%= link_to 'Edit', edit_director_path(@director), class: 'btn btn-secondary' %>
<%= link_to 'Back', directors_path, class: 'btn btn-primary' %>
